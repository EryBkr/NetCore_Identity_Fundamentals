@model AppUser
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

    <table class="table table-hover table-sm">
        <tr>
            <th>Name:</th>
            <td>@Model.UserName</td>
        </tr>
        <tr>
            <th>Email:</th>
            <td>@Model.Email</td>
        </tr>
        <tr>
            <th>Telefon:</th>
            <td>@Model.PhoneNumber</td>
        </tr>
        <tr>
            <th>Picture:</th>
            <td>@Model.PictureURL</td>
        </tr>
    </table>
